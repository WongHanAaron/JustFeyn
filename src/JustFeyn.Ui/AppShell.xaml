<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="JustFeyn.Ui.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:JustFeyn.Ui"
    xmlns:study="clr-namespace:JustFeyn.Ui.Pages.Study"
    Shell.FlyoutBehavior="Flyout"
    >

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="mainpage" />
    <ShellContent
        Title="Video Study"
        ContentTemplate="{DataTemplate study:VideoStudyPage}"
        Route="videostudy"
        />
</Shell>
